{
  "name": "acrobot",
  "version": "0.0.1",
  "description": "slack chatbot for acronym lookups",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "postinstall": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc -p .",
    "tsc:watch": "tsc -p . -w",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src}/**/*.ts\"",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "npm-run-all prettier clean tsc",
    "nodemon": "FILE=entries.csv nodemon -w dist",
    "start:dev": "npm-run-all --parallel tsc:watch nodemon"
  },
  "keywords": ["chatbot", "slack", "acronym", "acrobot"],
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "license": "ISC",
  "dependencies": {
    "csvtojson": "^1.1.7",
    "fuse.js": "^3.0.5",
    "json2csv": "^3.10.0",
    "lodash.sortby": "^4.7.0",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "restify": "^5.0.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/csvtojson": "^1.1.4",
    "@types/lodash": "^4.14.71",
    "@types/restify": "^5.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "tsc": "^1.20150623.0"
  }
}
